		"@babel/preset-env": "^7.10.4",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.1.0",
		"babel-loader": "^8.1.0",
		"casual": "^1.6.2",
		"jest": "^26.1.0",
		"mongodb-memory-server": "6.6.1"
	},
	"engines": {
		"node": "14.11.0",
		"npm": ">=6.2.0"
