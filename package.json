{
	"name": "devpolls.api",
	"version": "1.0.0",
	"description": "A graphql based api endpoint for DevPolls web app",
	"main": "src/server.js",
	"scripts": {
		"start": "pm2-runtime start ecosystem.config.js",
		"format": "prettier --write \"src/**/*.js\"",
		"test": "jest --testMatch \"**/?(*.)+(spec|test).[tj]s?(x)\" --detectOpenHandles --forceExit --runInBand",
		"test:coverage": "jest --coverage --testMatch '**/?(*.)+(spec|test).[tj]s?(x)'  --detectOpenHandles --forceExit --runInBand",
		"lint": "eslint src/**/**/*.js"
	},
	"dependencies": {
		"@bearer/node-agent": "3.1.3",
		"agenda": "3.1.0",
		"agendash": "^1.0.0",
		"apollo-server-express": "2.24.1",
		"axios": "0.21.1",
		"colors": "^1.4.0",
		"connect-timeout": "^1.9.0",
		"cors": "^2.8.5",
		"dotenv": "8.2.0",
		"error-to-json": "^2.0.0",
		"express": "^4.17.1",
		"firebase": "^8.3.1",
		"firebase-admin": "^9.8.0",
		"graphql": "15.1.0",
		"helmet": "^3.23.3",
		"mailgun-js": "^0.22.0",
		"moment": "2.27.0",
		"moment-timezone": "0.5.31",
		"mongodb": "3.6.2",
